@model IEnumerable<clinic_api.Models.Doctore>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreCompleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cedula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contraseña)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diploma)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Perfil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EspecialidadFkNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InformacionMedicaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contraseña)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diploma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Perfil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EspecialidadFkNavigation.IdEspecialidades)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InformacionMedicaNavigation.IdinformacionesMedicas)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDoctores">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDoctores">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDoctores">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
